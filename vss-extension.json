{
    "manifestVersion": 1,
    "id": "branch-magic5",
    "version": "0.1.44",
    "name": "Branch Magic",
    "scopes": ["vso.code_manage"],
    "description": "Merge master into your local branch from a pull request.",
    "publisher": "aaronyzhou",
    "icons": {
        "default": "static/images/logo.png"
    },
    "public": "false",
    "targets": [
        {
            "id": "Microsoft.VisualStudio.Services"
        }
    ],
    "tags": [
        "pull request", "branch", "code"
    ],
    "content": {
        "details": {
            "path": "overview.md"
        }
    },
    "links": {
        "home": {
            "uri": "https://github.com/aaronyzhou/vsts-extension-branch-magic"
        },
        "getstarted": {
            "uri": "https://github.com/aaronyzhou/vsts-extension-branch-magic"
        },
        "learn": {
            "uri": "https://github.com/aaronyzhou/vsts-extension-branch-magic"
        },
        "support": {
            "uri": "https://github.com/aaronyzhou/vsts-extension-branch-magic"
        },
        "repository": {
            "uri": "https://github.com/aaronyzhou/vsts-extension-branch-magic"
        },
        "issues": {
            "uri": "https://github.com/aaronyzhou/vsts-extension-branch-magic"
        }
    },
    "branding": {
        "color": "rgb(220, 235, 252)",
        "theme": "light"
    },
    "files": [
        {
            "path": "lib",
            "addressable": true
        },
        {
            "path": "dist",
            "addressable": true
        },
        {
            "path": "static/index.html",
            "addressable": true
        }
    ],
    "categories": [
        "Code"
    ],
    "contributions": [
      {
        "id": "branch-magic-source-tree",
        "type": "ms.vss-web.action",
        "description": "branch magic",
        "targets": [
            "ms.vss-code-web.source-tree-item-menu"
        ],
        "properties": {
            "text": "branch magic",
            "title": "ms.vss-code-web.branch-magic",
            "uri": "static/index.html"
        }
      },
      {
        "id": "branch-magic-pull-request-menu",
        "type": "ms.vss-web.action",
        "description": "branch magic",
        "targets": [
            "ms.vss-code-web.pull-request-action-menu"
        ],
        "properties": {
            "text": "branch magic",
            "title": "ms.vss-code-web.branch-magic",
            "uri": "static/index.html"
        }
      }
    ]
}
